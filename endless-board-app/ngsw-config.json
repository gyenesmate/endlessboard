{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/endless-board-app/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/endless-board-app/favicon.ico",
          "/endless-board-app/index.csr.html",
          "/endless-board-app/index.html",
          "/endless-board-app/manifest.webmanifest",
          "/endless-board-app/*.css",
          "/endless-board-app/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ]
}
